<div id="<%= dom_id habit_entry %>">
  <p>
    <strong>Habit:</strong>
    <%= @habit.name %>
  </p>

  <p>
    <strong>Entry date:</strong>
    <%= habit_entry.entry_date.strftime("%m-%d-%Y") %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= habit_entry.status %>
  </p>

    <p>
    <strong>Note:</strong>
    <%= habit_entry.note %>
  </p>

</div>
